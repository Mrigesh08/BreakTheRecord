	}

	.container {
		opacity: 0.9;
	}

	.inside {
		background-color: white;
		overflow: hidden;
		text-align: center;
	}

	.intro-box {
		height: 100vh;
		display: table;
		text-align: center;
		width: 100%;
	}

	.intro-box2 {
		height: 400px;
		display: table;
		text-align: center;
		width: 100%;
	}

	.intro {
		display: table-cell;
		vertical-align: middle;
	}

	.arrow_effect {
		position: absolute;
		z-index: 4;
		left: 49%;
		bottom: 30px;
		color: white !important;
		width: auto;
	}

	.arrow {
		margin: 0 !important;
		color: white;
		font-size: 2em;
	}

	@keyframes arrow1 {
		0% {
			opacity: 1;
		}
		25% {
			opacity: 0.5;
		}
		33% {
			opacity: 0;
		}
		66% {
			opacity: 0;
		}
		75% {
			opacity: 0.5;
		}
		100% {
			opacity: 1;
		}
	}

	@keyframes arrow2 {
		0% {
			opacity: 0;
		}
		25% {
			opacity: 0.5;
		}
		33% {
			opacity: 1;
		}
		50% {
			opacity: 0.5;
		}
		66% {
			opacity: 0;
		}
		100% {
			opacity: 0;
		}
	}

	@keyframes arrow3 {
		0% {
			opacity: 0;
		}
		33% {
			opacity: 0;
		}
		50% {
			opacity: 0.5;
		}
		66% {
			opacity: 1;
		}
		75% {
			opacity: 0.5;
		}
		100% {
			opacity: 0;
		}
	}

	.arrow_1 {
		animation: arrow1 1000ms infinite;
	}

	.arrow_2 {
		animation: arrow2 1000ms infinite;
		margin-top: -20px !important;
	}

	.arrow_3 {
		animation: arrow3 1000ms infinite;
		margin-top: -20px !important;
	}

	@media screen and (max-width:768px) {
		.apogeelogo {
			height: 60px;
		}
		.bomb {
			font-size: 1.2em !important;
		}
		.mikelogo {
			width: 300px;
		}
		.t {
			font-size: 2em !important;
		}
		.text-1{
			font-size: 2.5em !important;
		}
	}
</style>
